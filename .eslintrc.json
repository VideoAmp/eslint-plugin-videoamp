{
    "env": {
        "amd": true,
        "es6": true,
        "jest": true,
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "no-unused-vars": ["error", { "varsIgnorePattern": "_" }]
    }
}
